package com.aoshi.domain;

public class AsAttachment {
    private Integer sysAttachmentId;

    private Integer type;

    private Integer itemId;

    private String attachmentUrl;

    public Integer getSysAttachmentId() {
        return sysAttachmentId;
    }

    public void setSysAttachmentId(Integer sysAttachmentId) {
        this.sysAttachmentId = sysAttachmentId;
    }

    public Integer getType() {
        return type;
    }

    public void setType(Integer type) {
        this.type = type;
    }

    public Integer getItemId() {
        return itemId;
    }

    public void setItemId(Integer itemId) {
        this.itemId = itemId;
    }

    public String getAttachmentUrl() {
        return attachmentUrl;
    }

    public void setAttachmentUrl(String attachmentUrl) {
        this.attachmentUrl = attachmentUrl == null ? null : attachmentUrl.trim();
    }
}