package com.aoshi.common.asenum;

public enum AccountTypeEnum {

	CONSUME(1, "消费"), INCOME(2, "收入"), POSTAL(3, "提现"), REFUND(4, "退款");

	private int id;
	private String remark;

	private AccountTypeEnum(int id, String remark) {
		this.id = id;
		this.remark = remark;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

}
