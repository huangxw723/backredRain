package com.aoshi.domain;

public class AsBeaconLink {
    private Integer beaconLinkId;

    private String beaconName;

    private Integer status;

    private String beaconDesc;

    private boolean prizeChoosed;//非数据库字段，用于页面判断

    private boolean beaconChoosed;//非数据库字段，用于页面判断
    
    private Integer perCount;

    public boolean isPrizeChoosed() {
        return prizeChoosed;
    }

    public void setPrizeChoosed(boolean prizeChoosed) {
        this.prizeChoosed = prizeChoosed;
    }

    public boolean isBeaconChoosed() {
        return beaconChoosed;
    }

    public void setBeaconChoosed(boolean beaconChoosed) {
        this.beaconChoosed = beaconChoosed;
    }

    public Integer getBeaconLinkId() {
        return beaconLinkId;
    }

    public void setBeaconLinkId(Integer beaconLinkId) {
        this.beaconLinkId = beaconLinkId;
    }

    public String getBeaconName() {
        return beaconName;
    }

    public void setBeaconName(String beaconName) {
        this.beaconName = beaconName == null ? null : beaconName.trim();
    }

    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }

    public String getBeaconDesc() {
        return beaconDesc;
    }

    public void setBeaconDesc(String beaconDesc) {
        this.beaconDesc = beaconDesc == null ? null : beaconDesc.trim();
    }

	public Integer getPerCount() {
		return perCount;
	}

	public void setPerCount(Integer perCount) {
		this.perCount = perCount;
	}
    
}