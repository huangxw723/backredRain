<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>注册</title>
	<link rel="stylesheet" type="text/css" href="css/base.css">
	<link rel="stylesheet" type="text/css" href="css/common.css">
	<link rel="stylesheet" type="text/css" href="css/tenant_register.css">
</head>
<body>
	<div class="register">
		<div class="detailed_info">
			<div class="seller_type">
				<span>商户类型:</span>
				<div class="select_icon" data-userType="1">
					<i></i>
					<span>商场</span>
				</div>
				<div class="select_icon" data-userType="2">
					<i></i>
					<span>店铺</span>
				</div>
			</div>
			<div class="mb20">
				<span>用户名:</span>
				<input type="text" name="" placeholder="用户名" class="user_name" />
			</div>
			<div class="mb20">
				<span>设置密码:</span>
				<input type="password" name="" placeholder="6到20位" class="password" />
			</div>
			<div class="mb20">
				<span>预留手机号码:</span>
				<input type="text" name="" placeholder="用于找回密码" class="phone" />
			</div>
			<input type="button" name="" value="注册" class="register_button">
		</div>
	</div>
	<!-- Loading -->
	<div class="loader">
	    <div class="loader-inner">
	        <div class="loader-line-wrap">
	            <div class="loader-line"></div>
	        </div>
	        <div class="loader-line-wrap">
	            <div class="loader-line"></div>
	        </div>
	        <div class="loader-line-wrap">
	            <div class="loader-line"></div>
	        </div>
	        <div class="loader-line-wrap">
	            <div class="loader-line"></div>
	        </div>
	        <div class="loader-line-wrap">
	            <div class="loader-line"></div>
	        </div>
	    </div>
	</div>
</body>
</html>
<script src="js/jquery-1.11.1.min.js"></script>
<script src="js/template-debug.js"></script>
<script src="js/public-url.js"></script>
<script src="js/alertPopShow.js"></script>
<script src="js/tenant_register.js"></script>
