.bt-2 li,.bt-3 li{height:225px;text-align:center;float:left;padding-top:69px;}
.bt-2 li{margin-bottom:10px;}
.bt-2 li:nth-child(1){background:#4ab940;width:210px;}
.bt-2 li:nth-child(2){background:#00aed0;width:288px;margin-left:10px;}
.bt-2 li:nth-child(3){background:#ea4043;width:352px;margin-left:10px;}
