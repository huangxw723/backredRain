/***************
    登录注册
****************/
.login-h li{display:inline-block;margin-right:10px;}
.login-img{float:left;}
#main1.login-pg{margin-top:150px;}
.login{border:1px solid #ccc;padding:10px 30px;float:left;width:348px;color:#666;margin-left:70px;}
.login .p1 span:nth-child(1){font-size:18px;color:#e0007d;}
.login .p1 span:nth-child(2){float:right;}
.inp-list{background:#fff;border:1px solid #ccc;margin:0px auto;margin-top:12px;height:30px;line-height:35px;font-size:13px;width:340px;color:#777;padding:5px 0 5px 5px;}
.inp-list input{height:30px;border:none;outline:none;vertical-align:top;}
.inp-list label{padding:0 5px;}
.inp-list .fa{font-size:20px;}
.inp-list1{margin-top:12px;}
.inp-list1 input{width:224px;height:34px;padding:0 6px;outline:none;border:1px solid #ccc;}
.setin .inp-list1 input{width:160px;}

/*.inp-list1 input:focus{
    border: 1px solid #e0007d;
}*/
.inp-list1 button{width:100px;height:36px;margin-left:10px;padding:6px 0;}
.setin .inp-list1 button{height:29px;padding:4px 0;margin-right:5px;}
.login input[type="submit"]{width:100%;padding:8px 5px;margin-top:40px;outline:none;background:#e0007d;color:#fff;border:none;border-radius:4px;font-size:16px;}
.login input[type="submit"]:focus,.login input[type="submit"]:hover{background:#ca0063;border-color:#ca0063;color:#fff;}
.login .p2{margin-left:-4px;}
.login .p2 input[type="checkbox"]{vertical-align:middle;}
.login .r{float:right;}
.login-list2{margin-top:20px;}
.login-list2 li{display:inline-block;margin-right:10px;}
.login-fd{position:fixed;bottom:50px;margin:0px auto;width:100%;text-align:center;color:#777;}











