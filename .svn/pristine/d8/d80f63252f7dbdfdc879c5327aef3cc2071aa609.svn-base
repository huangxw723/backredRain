body{
    padding-top: 5px;
    font: 12px/1.5 tahoma,arial,Hiragino Sans GB,WenQuanYi Micro Hei,\5FAE\8F6F\96C5\9ED1,\5B8B\4F53,sans-serif;
    line-height: 1.7em;
}
a{color: #141414;text-decoration: none;}
a:hover{text-decoration: none;}
/*img{width: 100%;}
*/.page-w{ width: 1240px;
    margin:auto;}
.page-w1{ width: 1110px;
    margin:auto;}
ul{list-style: none;padding: 0;}
.color-main,.color-main a{
	color: #e0007d;
}
.dropdown .dropdown-toggle{
   z-index: 100000;
   background: #fff;
   padding:10px 2px;
}

.dropdown-menu{
	min-width: 0px;
}
 button.color-main{ color: #fff; background: #e0007d; border-color: #e0007d; border: none;}
 button.color-main:focus,  button.color-main:hover{ background: #ca0063; border-color: #ca0063; color: #fff; }
.mg-last{
	margin: 0;
}
.margin{
	margin: 5px 0;
}
.tabcontent { display: none; }
.tabcontent1 { display: none; }
.tabcontent2 { display: none; }
.tabcontent3 { display: none; }
#main{
	width: 1240px;
	margin: 30px auto;
}
#main1{
	width: 1110px;
	margin: 30px auto;
}

#main2{
    width: 1110px;
    margin: 150px auto;
}
/*aside*/
#aside { position: fixed; right: 0; top: 0px; z-index: 101; width: 36px; height: 100%; font-family: "Microsoft YaHei"; color: #fff; }
#aside li { position: relative; width: 36px; }
#aside .aside-nav { position: relative; z-index: 101; height: 600px; background-color: #262626; margin-top:200px; margin-bottom:200px;}
#aside .aside-nav .aside-list { margin-top: -280px; height: 400px; position: absolute; top: 57%; right: 0; }
.i-sidebarcom-asset, .i-sidebarcom-brand, .i-sidebarcom-cart, .i-sidebarcom-fav, .i-sidebarcom-my, .i-sidebarcom-nub, .i-sidebarcom-survey, .i-sidebarcom-top { display: block; margin: 20px auto; width: 36px; height: 40px; background-image: url(../images/index/sprite.png); background-repeat: no-repeat; background-color: #262626; cursor: pointer; }
.i-sidebarcom-my { background-position: -82px 0; }
.i-sidebarcom-asset { background-position: -164px 0; }
.i-sidebarcom-brand { background-position: -41px -45px; }
.i-sidebarcom-fav { margin-bottom: 30px; background-position: -123px -45px; }
.sidebarcom-cart-bar { height: 125px; cursor: pointer; }
.i-sidebarcom-cart { margin: -1px auto 0; width: 36px; border-top: 1px solid #4c4c4c; background-position: -164px -130px; }
.sidebarcom-cart-text { margin: 0 auto; width: 14px; line-height: 18px; word-wrap: break-word; word-break: nomal; font-size: 14px; color: #d1bdc6; }
.i-sidebarcom-nub { margin-top: 5px; width: 20px; height: 21px; line-height: 22px; padding-bottom: 8px; border-bottom: 1px solid #4c4c4c; background-position: -82px -90px; text-align: center; color: #fff; }
/*首页header*/
.tab_child{display: none;}
.index-h { width: 1240px; margin: 0px auto; text-align: center; position: relative; font-size: 12px; height: 30px; }
.index-h li { display: inline-block; margin-right: 8px; }
.index-h .w2{    position: absolute;
    text-align: center;
    display: none;
    padding: 9px 5px;
    left: -1px;
    top: 16px;
    border: 1px solid #ddd;
    background: #fff;
    z-index: -12;}
.index-h .phone{
	position: relative;
    border: 1px solid #ddd;
    z-index: 10111111;
    background: #fff;
}
.index-h .phone a{
	background: #fff;
    padding: 10px 20px;
}
.fa-folder-open { color: #e0007d; }
.page-w.h-form { margin-top: 20px; }
.h-form .text-left { margin-left: 30px; color: #e0007d; }
.h-form .text-left h1 { margin: -8px 0 0 0px; }
.h-form .num { position: absolute; left: 24px; top: -11px; color: #fff; width: 15px; height: 15px; background: #e0007d; border-radius: 50px; text-align: center; vertical-align: middle; }
.h-form .fa-search { position: absolute; top: 10px; left: 10px; z-index: 101; color: #e0007d; font-size: 16px; }
.h-form .text-right.col-md-2{z-index: 11;} 
.h-form .text-right .cart-r { color: #fff; background: #e0007d; width: 120px; padding: 6px;z-index: 101; }
.h-form .text-right .cart-r:hover{
	background: #fff;
    color: #e0007d;
    border: 1px solid #e0007d;
    z-index: 101;
    border-bottom-color: transparent;
}
.h-form .text-right .pull-right { margin-top: 3px; padding-right: 3px; margin-left: 4px; }
.h-form .fa-caret-down { margin-right: 3px; }
.h-form .form-control { border-color: #e0007d; padding-left: 30px; }
.h-form button { color: #fff; background: #e0007d; border-color: #e0007d; }
.h-form button:focus, .h-form button:hover { background: #ca0063; border-color: #ca0063; color: #fff; }
.h-form .form-control:focus { border-color: #e0007d; outline: 0; -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(224, 0, 125, 0.6); box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(224, 0, 125, 0.6); }
.h-form ul { padding-left: 0px; float: left; }
.h-form li { display: inline-block; padding-right: 10px; }
.h-form .cart-hover{width: 225px;background: #fff;position: absolute;left: -95px;z-index: -1;top: 37px;color: #777;border: 1px solid #e0007d;padding: 20px;}
.nav-brand { background: #e0007d; margin-top: 15px; }
.nav-brand a { color: #fff; }
.nav-pills>li.active>a { background: #ca0063; }
.nav-pills>li.active>a:hover, .nav-pills>li.active>a:focus, .nav-pills>li>a:hover, .nav-pills>li>a:focus { background: #ca0063; color: #fff; }
.index-hot h3 { font-size: 14px; font-weight: normal; }
.index-hot ul { margin-bottom: 0px; }
.index-hot .index-hot-1 { border: 1px solid #ddd; }
.index-hot .index-hot-1 li { width: 33.33333333%; float: left; }
.index-hot .index-hot-1 li ul li { width: 49%; }
.index-hot li+li { border-left: 1px solid #ddd; }

/*footer*/
#footer { border-top: 2px solid #e0007d; margin: 30px auto; text-align: center;margin-bottom:50px; }
#footer .fh, #footer .fd { width: 1240px; margin: 50px auto; text-align: center; }
#footer .fh span { margin-bottom: 30px; }
#footer .fh ul  li { float: left; padding: 10px 60px; margin: 0px 10px; text-align: center; width: 169px; }
#footer .fh ul li ul { margin-top: 40px; height: 120px; width: 169px; border-right: 1px solid #ddd; }
#footer .fh ul li ul li:first-child { font-weight: bold; }
#footer .fh ul li .last { border-right-color: transparent; }
#footer .fh ul  li ul li { float: none; margin: 0px; text-align: left; padding: 0; }
#footer li img { height: 52px; width: 52px; display: block; }
#footer nav{background: #e0007d;height: 30px;text-align: center;margin: 0px auto;padding: 5px 0;}
#footer nav ul{margin: 0px auto;text-align: center;}
#footer nav li{
	display: inline-block;
	text-align: center;
	height: 15px;
	line-height: 15px;
	padding: 0px 5px;
}
#footer nav li+li{
	border-left: 2px solid #fff;
}
#footer nav li a{
	color: #fff;
}